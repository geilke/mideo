<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
    <Console name="Console">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{2} - %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="error">
      <AppenderRef ref="Console"/>
    </Root>
    <logger name="org.kramerlab.mideo.evaluation.JobCenter" level="info">
        <AppenderRef ref="Console"/>
    </logger>
    <logger name="org.kramerlab.mideo.evaluation.JobDescription" level="info">
        <AppenderRef ref="Console"/>
    </logger>
  </Loggers>
</Configuration>
